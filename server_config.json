{
  "mysql": {
    "connections":1000,
    "host":"localhost",
    "user":"root",
    "password_file":"mysql_password.txt"
  },
  "cookies": {
    "server_hashes": 10000,
    "client_hashes": 10
  },
  "rate_limits": {
    "api": {
      "reset_time": 10000,
      "max_without_session": 20,
      "max_with_session": 60,
      "max_per_account": 30
    },
    "user": {
      "reset_time": 30000,
      "max": 60
    }
  },
  "logs": {
    "level": 5
  },
  "ssl": {
    "privateKey": "/etc/letsencrypt/live/ipost.tk/privkey.pem",
    "certificate" : "/etc/letsencrypt/live/ipost.tk/cert.pem"
  },
  "disallow_proxies_by_headers": true
}
